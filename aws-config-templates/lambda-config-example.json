{
  "FunctionName": "twitter-user-service",
  "Runtime": "java17",
  "Role": "arn:aws:iam::ACCOUNT-ID:role/twitter-microservices-lambda-role",
  "Handler": "com.arep.twitter.userservice.StreamLambdaHandler::handleRequest",
  "Code": {
    "ZipFile": "fileb://deployment-jars/user-service-1.0.0.jar"
  },
  "Description": "User Service microservice for Twitter Clone",
  "Timeout": 30,
  "MemorySize": 1024,
  "Architectures": ["x86_64"],
  "Environment": {
    "Variables": {
      "SPRING_PROFILES_ACTIVE": "lambda",
      "MONGODB_URI": "mongodb+srv://...",
      "COGNITO_ISSUER_URI": "https://cognito-idp.us-east-1.amazonaws.com/us-east-1_3UE2M9esT"
    }
  }
}
